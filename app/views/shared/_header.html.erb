<header class="header-logged-in">
  <div class="container">
    <div class="language-links ">
      <small>
        <%= link_to "English", "#" %>
      </small>
      <small>
        <%= link_to "Help Learners", "http://community.digitallearn.org", class: "practice-link" %>
      </small>
    </div>
    <div class="logo-link-container">
      <%= link_to root_path do %>
        <div class="logo">
          <div class="logo-icon">
            <%= image_tag "dl_logo" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</header>
